FactoryGirl.define do
  factory :user do |f|
  f.sequence(:name) { |n| "test#{n}" }
  f.sequence(:username) { |n| "test#{n}" }
  f.sequence(:interests) { |n| "food" }
  f.sequence(:password) { |n| "test123" }
  f.sequence(:password_confirmation) { |n| "test123" }
 end
end
